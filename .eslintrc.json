{
  "parser": "babel-eslint",
  "extends": [
    "airbnb",
    "plugin:react-native/all",
    "plugin:jest/recommended",
  ],
  "plugins": [
    "jest",
    "react-native",
    "require-jsdoc"
  ],
  "env": {
    "jest/globals": true,
    "react-native/react-native": true
  },
  "rules": {
    "require-jsdoc": 2,
    "valid-jsdoc": 2
  }
}